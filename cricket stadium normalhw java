import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        int a,b;
            Scanner s=new Scanner(System.in);
        a=s.nextInt();
        b=s.nextInt();
        int r1=2*(a+b);
        System.out.printf("%d",r1);
        int r2=a*b;
        System.out.printf("\n%d",r2);
    }
}
