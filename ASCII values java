import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
            char ch;
        Scanner s=new Scanner(System.in);
        ch=s.next().charAt(0);
    if(ch >= 'A' && ch <= 'Z')
       System.out.printf("Upper");
    else if(ch >= 'a' && ch <= 'z')
       System.out. printf("Lower");
    else if(ch >= '0' && ch <= '9')
        System.out.printf("Number");
    else
        System.out.printf("Symbol");
    }
}
